{
  "name": "Netflixify",
  "version": "0.1",
  "description": "Finds Netflix titles in the web page you're on and pops up a link window.",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": [ "jquery-1.12.0.min.js", "contentscript.js" ]
    }
  ],
  "manifest_version": 2
}
